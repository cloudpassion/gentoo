UQD_USE1="X minimal alsa pulseaudio nvenc xinerama"
UQD_USE2="vaapi vdpau vulkan bluetooth dbus wayland opengl opencl"
UQD_DB_USE="" #postgres sqlite odbc freetds mysql oci8"

